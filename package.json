{
  "name": "@coffeexdev/openclaw-xmtp",
  "version": "2026.2.16",
  "description": "OpenClaw XMTP channel plugin for E2E encrypted wallet-to-wallet messaging",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/coffeebot-agent/openclaw-xmtp.git"
  },
  "keywords": [
    "openclaw",
    "xmtp",
    "channel",
    "plugin",
    "e2e",
    "encrypted",
    "messaging",
    "wallet",
    "ethereum"
  ],
  "files": [
    "index.ts",
    "src/",
    "openclaw.plugin.json"
  ],
  "dependencies": {
    "@xmtp/agent-sdk": "^2.2.0",
    "viem": "^2.23.2",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "openclaw": ">=2026.2.15"
  },
  "devDependencies": {
    "openclaw": "latest",
    "vitest": "^3.1.1"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "xmtp",
      "label": "XMTP",
      "selectionLabel": "XMTP (E2E Encrypted DMs)",
      "docsPath": "/channels/xmtp",
      "docsLabel": "xmtp",
      "blurb": "E2E encrypted messaging via XMTP protocol; wallet-to-wallet.",
      "order": 101,
      "quickstartAllowFrom": true
    },
    "install": {
      "npmSpec": "@coffeexdev/openclaw-xmtp",
      "localPath": "extensions/xmtp",
      "defaultChoice": "npm"
    }
  },
  "scripts": {
    "test": "vitest run"
  }
}
